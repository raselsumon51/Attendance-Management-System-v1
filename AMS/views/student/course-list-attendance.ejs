<div class="container">
  <h4 class="mx-3">All Courses</h4>
  <table class="table">
    <thead>
      <tr>
        <th>Course Name</th>
        <th>Course Code</th>
        <th>Teacher</th>
      </tr>
    </thead>
    <tbody>
      <% courses.forEach(course => { %>
      <tr>
        <td><a href="/attendance/course-id/<%= course._id %>"><%= course.name %></a></td>
        <td><%= course.code %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
